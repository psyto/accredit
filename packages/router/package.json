    "@accredit/types": "workspace:*",
    "@solana/web3.js": "^1.98.0"
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.6.3",
    "vitest": "^2.1.0"
  },
  "license": "MIT"
}
